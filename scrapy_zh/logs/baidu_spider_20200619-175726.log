    function followUser(object) {
        var isLogin = false;
        if (!isLogin) {
            lgwdsshow();
            return;
        }
        var obj = $(object);
        var userId = obj.attr("data-userId");
        var userType = obj.attr("data-type");
        $.post("/www/followUser.msp", {"followUserId": userId,"userType":userType}, function (data) {
            if (data.resultCode == 1) {
                obj.removeClass("link").addClass("linked");
                obj.attr("onclick", "unFollowUser(this)");
            } else {
                obj.attr("onclick", "followUser(this)");
            }
        });
    }

    function unFollowUser(object) {
        var isLogin = false;
        if (!isLogin) {
            lgwdsshow();
            return;
        }
        var obj = $(object);
        var userId = obj.attr("data-userId");
        var userType = obj.attr("data-type");
        $.post("/www/unFollowUser.msp", {"followUserId": userId,"userType":userType}, function (data) {
            if (data.resultCode == 1) {
                obj.removeClass("linked").addClass("link");
