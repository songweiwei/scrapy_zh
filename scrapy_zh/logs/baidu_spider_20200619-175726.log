        if (adVal != null && adVal != '') {
            $.ajax({
                url: "//adpai.thepaper.cn/s?",
                data: "c=" + adVal + "&op=1",
                cache: false,
                crossDomain: true,
                success: function (xml) {
                    handleXmlRes(xml);
                },
                error: function () {
                    oldHeadAdProcess();
                    console.log("Head Ad request error!");
                    return;
                }
            });
        }
        vNewsProcess();

    });
