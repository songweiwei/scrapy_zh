            video.get(0).play();
        }
        // configure top pic ad
        picLink.attr("href", click);
        picImg.attr("src", httpToSlash(creative));
        // ad flag logic
        if (adFlagStr == 0) {
            cornLab.remove();
        }
        var clickAction = function () {
